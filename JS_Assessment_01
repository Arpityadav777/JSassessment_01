// Array to hold all created Employee NFTs
const employeeNFTs = [];

// Function to mint a new Employee NFT
function mintNFT(id, name, age, ethnicity, jobTitle, dressingStyle, salary) {
    const nft = {
        "ID": id,
        "Name": name,
        "Age": age,
        "Ethnicity": ethnicity,
        "Job Title": jobTitle,
        "Dressing Style": dressingStyle,
        "Salary": salary,
    };
    employeeNFTs.push(nft);
    console.log(`Successfully minted ${name} with ID: ${id}`);
}

// Function to list all minted Employee NFTs
function listNFTs() {
    for (let i = 0; i < employeeNFTs.length; i++) {
        console.log(employeeNFTs[i]);
    }
}

// Function to get the total number of Employee NFTs created
function getTotalSupply() {
    console.log(`Total number of NFTs created: ${employeeNFTs.length}`);
}

// Example usage of the functions
mintNFT("001", "Arx", "18", "Indian", "NFT Creator", "Y2K", "$0");
listNFTs();
getTotalSupply();
